{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./wwwroot/tsc",
    "sourceMap": true,
    "target": "es5",
    "types": [
      "node",
      "webpack"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
